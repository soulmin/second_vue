<import src="../../../common/head.wxml"/>
<import src="../../../common/foot.wxml" />
<view class="cover-contain">
  <template is="head" data="{{title:'cover-view'}}" />
    <view class="cover-body">
      <view class="cover-section">
        <map latitude="{{latitude}}" longitude='{{longitude}}' style="width:100%;height:500rpx;">
          <cover-view class="cover-view">
            <cover-view class="flex-wrp" style="flex-direction:row;">
              <cover-view class="box demo-text-1" ></cover-view>
              <cover-view class="box demo-text-2" ></cover-view>
              <cover-view class="box demo-text-3" ></cover-view>
            </cover-view>
          </cover-view>
        </map>
      </view>
    </view>
  <template is="foot" />
</view>